<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<filter>
		<filter-name>AFilter</filter-name>
		<filter-class>cn.daxiong.servlet.AFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>AFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
	<filter>
		<filter-name>BFilter</filter-name>
		<filter-class>cn.daxiong.servlet.BFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>BFilter</filter-name>
		<url-pattern>/AServlet</url-pattern>
		<dispatcher>FORWARD</dispatcher>
	</filter-mapping>
	
</web-app>











